<style-scheme name="Chocolate Ice Cream" id="blackleaf" version="1.0">
  <author>Hamad Al Marri</author>
  <description>Color scheme using Ice Cream color palette</description>

  <style name="text"	foreground="#EDF2C2" background="#172601" />
  <style name="selection"	foreground="#000000" background="#B1F02A" />
  <style name="cursor"	foreground="#00DEA4" />
  <style name="secondary-cursor"	foreground="#00DEA4" />
  <style name="current-line"	background="#274611" />
  <style name="line-numbers"	foreground="#A09BA5" background="#172601" />
  <style name="background-pattern"	background="#000000" />
  <style name="bracket-match"	foreground="#FFC800" bold="true" />
  <style name="bracket-mismatch"	foreground="#FF636E" background="#172601" bold="true" />
  <style name="right-margin"	foreground="#000000" background="#B1F02A" />
  <style name="search-match"	foreground="#000000" background="#B1F02A" bold="true" />
  <style name="def:comment"	foreground="#839004" />
  <style name="def:shebang"	bold="true" />
  <style name="def:doc-comment-element"	italic="true" />
  <style name="def:constant"	foreground="#B1F02A" />
  <style name="def:special-char"	foreground="#33DA7A" />
  <style name="def:statement"	foreground="#46EBE3" />
  <style name="def:operator"	foreground="#FDA954" />
  <style name="def:arith-operator"	foreground="#ECF22E" />
  <style name="def:preprocessor"	foreground="#FFC800" />
  <style name="def:error"	foreground="#FF636E" bold="true" />
  <style name="def:note"	foreground="#7C7C7C" bold="true" />
  <style name="def:underlined"	italic="true" underline="true" />
  <style name="def:identifier"	foreground="#30F077" />
  <style name="def:type"	foreground="#33DA7A" />
</style-scheme>
