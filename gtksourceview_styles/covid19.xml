<style-scheme name="COVID-19" id="covid19" version="1.0">
  <author>Hamad Al Marri</author>
  <description>Color scheme using COVID-19 color palette</description>

  <style name="text"	foreground="#F5E8E2" background="#470100" />
  <style name="selection"	foreground="#F5E8E2" background="#B80009" />
  <style name="cursor"	foreground="#F5E8E2" />
  <style name="secondary-cursor"	foreground="#f1f0f1" />
  <style name="current-line"	background="#570100" />
  <style name="line-numbers"	foreground="#856565" background="#470100" />
  <style name="background-pattern"	background="#470100" />
  <style name="bracket-match"	foreground="#E49CED" bold="true" />
  <style name="bracket-mismatch"	foreground="#F5E8E2" background="#B80009" bold="true" />
  <style name="right-margin"	foreground="#BEB0BE" background="#827082" />
  <style name="search-match"	foreground="#36292d" background="#fef5d7" bold="true" />
  <style name="def:comment"	foreground="#856565" />
  <style name="def:shebang"	bold="true" />
  <style name="def:doc-comment-element"	italic="true" />
  <style name="def:constant"	foreground="#8CABBD" />
  <style name="def:special-char"	foreground="#A1D2FF" />
  <style name="def:statement"	foreground="#E396D4" />
  <style name="def:operator"	foreground="#73E3FA" />
  <style name="def:arith-operator"	foreground="#F79463" />
  <style name="def:preprocessor"	foreground="#A1D2FF" />
  <style name="def:error"	foreground="#ff1846" bold="true" />
  <style name="def:note"	foreground="#D33682" bold="true" />
  <style name="def:underlined"	italic="true" underline="true" />
  <style name="def:identifier"	foreground="#99F0C0" />
  <style name="def:type"	foreground="#73E3FA" />
</style-scheme>
